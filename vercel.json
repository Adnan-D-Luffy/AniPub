{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [],
  "functions": {
    "backend/app.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}